#include <stdio.h> 
int main() {
	int integer1,integer2,integer3;
	printf("請輸入您的出生年份:");
	scanf("%d", &integer1);
	printf("請輸入您的出生月份:");
	scanf("%d", &integer2);
	printf("請輸入您的出生日期:");
	scanf("%d", &integer3);
	printf("您出生於%d",integer1);
	printf("年%d",integer2);
	printf("月%d",integer3);
	printf("日");
	return 0; 
		
}
